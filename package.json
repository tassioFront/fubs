{
  "name": "@fubs/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "docker:dev:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:prod:build": "docker-compose build workspaces-service",
    "docker:prod:up": "docker-compose up -d",
    "docker:prod:down": "docker-compose down",
    "docker:prod:logs": "docker-compose logs -f workspaces-service",
    "db:migrate": "cd apps/workspaces && npx prisma migrate dev",
    "db:migrate:deploy": "cd apps/workspaces && npx prisma migrate deploy",
    "db:generate": "cd apps/workspaces && npx prisma generate",
    "db:studio": "cd apps/workspaces && npx prisma studio",
    "db:seed": "cd apps/workspaces && npx prisma db seed",
    "dev": "nx serve workspaces",
    "build:ci": "yarn nx affected -t build --base=origin/main --head=HEAD --parallel=10 --verbose",
    "test:ci": "yarn nx affected -t test --base=origin/main --head=HEAD --parallel=10 --verbose",
    "lint:ci": "yarn nx affected -t lint --base=origin/main --head=HEAD --parallel=10 --verbose",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up workspaces-db-dev -d && nx serve workspaces",
    "docker:prod": "docker-compose up workspaces-db workspaces-service -d"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.11.0",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/eslint": "21.2.1",
    "@nx/eslint-plugin": "21.2.1",
    "@nx/jest": "21.2.1",
    "@nx/js": "21.2.1",
    "@nx/nest": "21.2.1",
    "@nx/node": "21.2.1",
    "@nx/web": "21.2.1",
    "@nx/webpack": "21.2.1",
    "@nx/workspace": "21.2.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "~18.16.9",
    "@types/passport-jwt": "^4.0.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.2.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "apps/*",
    "libs/*"
  ]
}
