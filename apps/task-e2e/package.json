{
  "name": "@fubs/tasks-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@fubs/tasks"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/task-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@fubs/tasks:build",
          "@fubs/tasks:serve"
        ]
      }
    }
  }
}
